<apex:page controller="WizardDemoController">
    <apex:form >
    	<apex:pageBlock title="New Contact Wizard: Step 3">
        	<apex:pageBlockButtons >
                <apex:commandButton action="{!pageThreePrevious}" value="Previous" />
            	<apex:commandButton action="{!pageThreeNext}" value="Next" />
            </apex:pageBlockButtons>
            <apex:pageBlockSection >
                <apex:outputText value="Phone"></apex:outputText>
                <apex:inputText value="{!phone}"></apex:inputText>
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>